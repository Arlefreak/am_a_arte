main
    width 50%
    margin auto
    overflow hidden
    max-width 650px
    @media screen and (max-width: 800px)
        width 80%
    div
        overflow hidden
    #instafeed
        min-height 80vh
        @media screen and (max-width: 800px)
            min-height 75vh
        li
            background white
            a
                color #3e3e3e
                text-decoration none
            p
                color black
                width 80%
                display block
                margin 10px auto 20px auto
                min-height 100px
                border-top 1px solid #e3e3e3
                padding 10px 0 0 0
                font-weight 300
                font-size 0.9em
            img
                max-width 100%
                padding 0 0 10px 0
